{
    "openapi": "3.1.0",
    "info": {
        "title": "Laravel",
        "version": "0.0.1"
    },
    "servers": [
        {
            "url": "http://localhost/api"
        }
    ],
    "paths": {
        "/auth/login": {
            "post": {
                "operationId": "authentication.login",
                "tags": [
                    "Authentication"
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "`TokenResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/TokenResource"
                                }
                            }
                        }
                    },
                    "422": {
                        "$ref": "#/components/responses/ValidationException"
                    }
                }
            }
        },
        "/auth/logout": {
            "post": {
                "operationId": "authentication.logout",
                "tags": [
                    "Authentication"
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "enum": [
                                                "logout_success"
                                            ]
                                        }
                                    },
                                    "required": [
                                        "message"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/emails": {
            "get": {
                "operationId": "successfulEmail.index",
                "tags": [
                    "SuccessfulEmail"
                ],
                "responses": {
                    "200": {
                        "description": "Array of `EmailResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/EmailResource"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/AuthenticationException"
                    }
                }
            },
            "post": {
                "operationId": "successfulEmail.store",
                "tags": [
                    "SuccessfulEmail"
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/EmailStoreRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "`EmailResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/EmailResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/AuthenticationException"
                    },
                    "422": {
                        "$ref": "#/components/responses/ValidationException"
                    }
                }
            }
        },
        "/emails/{email}": {
            "get": {
                "operationId": "successfulEmail.show",
                "tags": [
                    "SuccessfulEmail"
                ],
                "parameters": [
                    {
                        "name": "email",
                        "in": "path",
                        "required": true,
                        "description": "The email ID",
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "`EmailResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/EmailResource"
                                }
                            }
                        }
                    },
                    "404": {
                        "$ref": "#/components/responses/ModelNotFoundException"
                    },
                    "401": {
                        "$ref": "#/components/responses/AuthenticationException"
                    }
                }
            },
            "put": {
                "operationId": "successfulEmail.update",
                "tags": [
                    "SuccessfulEmail"
                ],
                "parameters": [
                    {
                        "name": "email",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/EmailUpdateRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "`EmailResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/EmailResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/AuthenticationException"
                    },
                    "422": {
                        "$ref": "#/components/responses/ValidationException"
                    }
                }
            },
            "delete": {
                "operationId": "successfulEmail.destroy",
                "tags": [
                    "SuccessfulEmail"
                ],
                "parameters": [
                    {
                        "name": "email",
                        "in": "path",
                        "required": true,
                        "description": "The email ID",
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No content"
                    },
                    "404": {
                        "$ref": "#/components/responses/ModelNotFoundException"
                    },
                    "401": {
                        "$ref": "#/components/responses/AuthenticationException"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "EmailResource": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "affiliate_id": {
                        "type": "string"
                    },
                    "envelope": {
                        "type": "string"
                    },
                    "from": {
                        "type": "string"
                    },
                    "subject": {
                        "type": "string"
                    },
                    "dkim": {
                        "type": "string"
                    },
                    "SPF": {
                        "type": "string"
                    },
                    "spam_score": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "raw_text": {
                        "type": "string"
                    },
                    "sender_ip": {
                        "type": "string"
                    },
                    "to": {
                        "type": "string"
                    },
                    "timestamp": {
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "affiliate_id",
                    "envelope",
                    "from",
                    "subject",
                    "dkim",
                    "SPF",
                    "spam_score",
                    "email",
                    "raw_text",
                    "sender_ip",
                    "to",
                    "timestamp"
                ],
                "title": "EmailResource"
            },
            "EmailStoreRequest": {
                "type": "object",
                "properties": {
                    "affiliate_id": {
                        "type": "integer"
                    },
                    "envelope": {
                        "type": "string"
                    },
                    "from": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "subject": {
                        "type": "string"
                    },
                    "dkim": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "maxLength": 255
                    },
                    "SPF": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "maxLength": 255
                    },
                    "spam_score": {
                        "type": [
                            "number",
                            "null"
                        ]
                    },
                    "email": {
                        "type": "string"
                    },
                    "raw_text": {
                        "type": "string"
                    },
                    "sender_ip": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "maxLength": 50
                    },
                    "to": {
                        "type": "string"
                    },
                    "timestamp": {
                        "type": "integer"
                    }
                },
                "required": [
                    "affiliate_id",
                    "envelope",
                    "from",
                    "subject",
                    "dkim",
                    "SPF",
                    "spam_score",
                    "email",
                    "raw_text",
                    "sender_ip",
                    "to",
                    "timestamp"
                ],
                "title": "EmailStoreRequest"
            },
            "EmailUpdateRequest": {
                "type": "object",
                "properties": {
                    "affiliate_id": {
                        "type": "integer"
                    },
                    "envelope": {
                        "type": "string"
                    },
                    "from": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "subject": {
                        "type": "string"
                    },
                    "dkim": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "maxLength": 255
                    },
                    "SPF": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "maxLength": 255
                    },
                    "spam_score": {
                        "type": [
                            "number",
                            "null"
                        ]
                    },
                    "email": {
                        "type": "string"
                    },
                    "raw_text": {
                        "type": "string"
                    },
                    "sender_ip": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "maxLength": 50
                    },
                    "to": {
                        "type": "string"
                    },
                    "timestamp": {
                        "type": "integer"
                    }
                },
                "required": [
                    "affiliate_id",
                    "envelope",
                    "from",
                    "subject",
                    "dkim",
                    "SPF",
                    "spam_score",
                    "email",
                    "raw_text",
                    "sender_ip",
                    "to",
                    "timestamp"
                ],
                "title": "EmailUpdateRequest"
            },
            "LoginRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "password"
                ],
                "title": "LoginRequest"
            },
            "TokenResource": {
                "type": "object",
                "properties": {
                    "access_token": {
                        "type": "string"
                    },
                    "token_type": {
                        "type": "string"
                    },
                    "expires_in": {
                        "type": "integer"
                    }
                },
                "required": [
                    "access_token",
                    "token_type",
                    "expires_in"
                ],
                "title": "TokenResource"
            }
        },
        "responses": {
            "ValidationException": {
                "description": "Validation error",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "description": "Errors overview."
                                },
                                "errors": {
                                    "type": "object",
                                    "description": "A detailed description of each field that failed validation.",
                                    "additionalProperties": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    }
                                }
                            },
                            "required": [
                                "message",
                                "errors"
                            ]
                        }
                    }
                }
            },
            "AuthenticationException": {
                "description": "Unauthenticated",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "description": "Error overview."
                                }
                            },
                            "required": [
                                "message"
                            ]
                        }
                    }
                }
            },
            "ModelNotFoundException": {
                "description": "Not found",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "description": "Error overview."
                                }
                            },
                            "required": [
                                "message"
                            ]
                        }
                    }
                }
            }
        }
    }
}